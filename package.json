{
  "name": "projectmirror-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "cole": "cd apps/cole && npm start",
    "companion": "cd apps/companion && npm start",
    "dev:cole": "cd apps/cole && npm start",
    "dev:companion": "cd apps/companion && npm start",
    "ios:cole": "cd apps/cole && npm run ios",
    "ios:companion": "cd apps/companion && npm run ios",
    "android:cole": "cd apps/cole && npm run android",
    "android:companion": "cd apps/companion && npm run android"
  }
}
